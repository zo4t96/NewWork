@model IEnumerable<MainWork.ViewModels.CSearchResult>

@{
    if (ViewBag.ajax != null)
    {
        @(Layout = null);
    }
    else
    {
        <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    }
}
<body>
    @foreach (var item in Model)
    {
        <div>專輯名稱:@item.album.fAlbumName</div>
        <div>@item.product.fProductName</div>
        <div>========================================</div>
    }
</body>

