@using MainWork.Models

<!doctype html>
<html lang="en">
<head>
    <title>Title</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    @* ↓主要用來讀取一些特殊icon *@
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <!--原始style-->
    <style>
        * {
            margin: 0;
            padding: 0;
            font-family: "微軟正黑體";
            color: white;
        }

        li {
            list-style-type: none;
            margin: 5px;
        }

        .header {
            position: fixed;
            width: 100%;
            height: 70px;
            background-color: #0b0b0b;
            z-index: 10;
        }

        .header-logo {
            position: absolute;
            top: 10px;
            left: 10px;
            height: 50px;
            width: 180px;
            padding-left: 40px;
            padding-top: 3px;
            font-size: 25px;
            background: #0b0b0b;
        }

        .header-logo-text {
            color: white;
        }

        .header-button {
            position: absolute;
            top: 15px;
            right: 16px;
        }

        #container {
            position: relative;
            top: 70px;
            left: 0; /*將子元素的定位到具體的參照對象*/
            width: 100%;
        }

        .sidebox {
            width:200px;
            position: fixed;
            top: 70px;
            background-color: #040404;
            color: white;
        }

        .mainbox {
            width: 100%;
            height: calc(100%-70px);
            min-height: 1600px;
            border-left: 200px solid #040404; /*設置右邊框寬度為XXXpx像素，同等於sidebox的寬度*/
            background-color: #191919;
            margin-bottom: 110px;
        }

        #footer {
            position: fixed;
            bottom: 0;
            left: 0;
            width: 100%;
            color: white;
            background-color:#252525;
            box-shadow: 0 0 2em black;
        }

        /*額外更新左邊選單*/
        .menuItem{
            width:190px;
            font-weight:600;
            font-size:14px;
            text-decoration:none;
            border-radius:5px;
            transition:0.4s;
        }
        .menuItem:hover{
            background-color:#818181;
        }
        .menuItem .fa{
            font-size:20px;
            margin:10px 20px;
        }
        .menuItem span{
            display:inline-block;
        }
    </style>

    <!--搜尋條style-->
    <style>
        .searchBar {
            position: relative;
            left: 250px;
            margin: 15px;
            width: 25%;
            min-width: 200px;
        }
        /*不用id變不了bootstrap的文字顏色*/
        #advancedBtn {
            color: whitesmoke;
        }

        .advancedPanel {
            display: none;
            width: 530px;
            border-radius: 10px;
            padding: 5px 20px 20px 20px;
            background-color: #040404;
            background-image: linear-gradient(#474747,#040404);
            opacity: 0;
            transition: 0.1s;
            box-shadow: 1px 1px 2px #818181;
        }

        .advancedItem {
            display: inline-flex;
            flex-wrap: wrap;
            width: 140px;
            margin: 10px;
        }

        .ui-menu-item-wrapper,
        option {
            color: black;
        }

        .advancedSelect {
            margin: 10px;
        }

        .selectedItem,
        .selectableItem,
        .itemAdd {
            display: inline-block;
            border-radius: 5px;
            border: 1px solid #212121;
            padding: 5px;
            margin: 2px 1px;
            color: white;
            font-weight: 600;
        }

        .selectedItem {
            display: none;
        }

        .itemAdd {
            padding: 5px 10px;
        }

        .selectableItem,
        .selectedItem,
        .itemAdd {
            cursor: pointer;
        }

            .itemAdd:hover {
                background-color: rgba(6, 78, 137, 0.90)
            }

        .selectableItemPanel {
            display: none;
            position: absolute;
            width: 90%;
            min-width: 200px;
            border-radius: 5px;
            background-color: rgba(0, 0, 0, 0.8);
            transition: 0.1s;
            box-shadow: 1px 1px 2px #525252;
        }

        .adSwitch {
            opacity: 1;
            display: block;
        }

        .searchPanel {
            width: 100%;
            text-align: right;
        }

        .adSearchBtn {
            background-image: linear-gradient(to bottom right, #22b7e8,#1347d9);
            margin-right: 10px;
        }
    </style>

    <!--播放器style-->
    <style>

        #playerPanel {
            position: relative;
            width: 100%;
            min-width: 1200px;
            height: 110px;
        }

        #titlePanel {
            position: absolute;
            left: 100px;
            width: 200px;
            height: 80px;
            margin: 10px;
            background: none;
        }

        img.title {
            width: 80px;
            height: 80px;
            margin: 2px 10px;
        }

        #playerfigure {
            position: absolute;
            width: 100px;
            margin: 2px;
            color: white;
        }

        #playerfigcaption {
            margin: 1px;
            text-align: center;
        }

        .description {
            font-size: 12px;
            overflow: hidden;
        }

        #singer {
            color: rgb(199, 194, 194);
            font-style: italic;
            font-size: 10px;
        }

        #btnPanel {
            position: relative;
            top: 3px;
            width: 400px;
            margin: auto;
            text-align: center;
        }

        #progressPanel {
            position: relative;
            width: 55%;
            margin: 5px auto;
            text-align: center;
        }

        #volumePanel {
            position: absolute;
            right: 0px;
            top: 40px;
            width: 160px;
        }

        .sound {
            width: 30px;
            height: 22px;
            filter: invert();
        }

        #volumeBar {
            -webkit-appearance: none;
            border-radius: 50px;
            width: 100px;
            height: 5px;
            outline: none;
        }

            #volumeBar:hover,
            #progress:hover,
            .sound:hover {
                cursor: pointer;
            }

        #progress {
            -webkit-appearance: none;
            border-radius: 50px;
            top: 50px;
            width: 80%;
            height: 5px;
            /*下面這個不關掉的話選取進度條時會出現藍框*/
            outline: none;
            margin: auto;
            transition: 0.2s;
        }

        input::-webkit-slider-thumb {
            -webkit-appearance: none;
            position: relative;
            width: 11px;
            height: 11px;
            background: rgb(201, 201, 201);
            border-radius: 50%;
            transition: 0.2s;
        }

        input::-webkit-slider-runnable-track {
            transition: 0.5s;
        }

        input::-webkit-slider-thumb:hover {
            width: 15px;
            height: 15px;
        }

        .btn:hover {
            background: skyblue;
            cursor: pointer;
        }

        .btn:active {
            background: silver;
        }

        .btn img {
            width: 30px;
            height: 22.5px;
            margin: 10px;
            /*下面是色彩反轉的濾鏡效果(把黑圖片變白)*/
            filter: invert();
        }
    </style>

</head>
<body>
    <div class="header">
        <div class="header-logo">
            <a href="#"><img @*src=""*@ alt=""></a>
            <button class="navbar-toggler header-logo-text" type="button">LOGO</button>
        </div>

        @* 搜尋條--套用bootstrap(input-group) *@
    <div class="searchBar">
        @* 基本搜尋條(結合進階搜尋鍵) *@
        <div class="input-group">
            <input class="searchtxt form-control" type="text" placeholder="搜尋歌曲..." name="basicSearch">
            <div class="input-group-append">
                <div class="btn btn-outline-secondary" id="advancedBtn">進階搜尋</div>
                <div class="btn btn-outline-secondary searchBtn">
                    <i class="fa fa-search"></i>
                </div>
            </div>
        </div>

        @* 進階搜尋面板 *@
        <div class="advancedPanel">
            <div class="advancedItemGroup">
                <div class="advancedItem">
                    歌曲名稱
                    <input type="text" class="advancedtxt form-control" id="adSong" />
                </div>
                <div class="advancedItem">
                    專輯名稱
                    <input type="text" class="advancedtxt form-control" id="adAlbum" />
                </div>
                <div class="advancedItem">
                    歌手
                    <input type="text" class="advancedtxt form-control" id="adSinger" />
                </div>
                <div class="advancedItem">
                    團體名稱
                    <input type="text" class="advancedtxt form-control" id="adGroup" />
                </div>
                <div class="advancedItem">
                    作曲者
                    <input type="text" class="advancedtxt form-control" id="adComposer" />
                </div>
                <div class="advancedItem">
                    發行類別
                    <select class="advancedtxt form-control" id="adType">
                        <option value="0">不指定</option>
                        @* 網頁初始化時會在這裡更新搜尋的類別 *@
                    </select>
                </div>
            </div>

            @* 這邊放自選類別 *@
            <div class="typeSelectGroup">
                @* 選擇想要的類別 *@
                <div class="typeSelect advancedSelect">
                    偏好的曲風
                    <div class="advancedBlock">
                        @* 網頁初始化時會在這裡更新曲風與增加鈕 *@
                    </div>

                    @* 點選+號後會出現的type選擇表，位置應在+號正下方 *@
                    <div class="selectableItemPanel">
                        @* 網頁初始化時會在這裡更新曲風 *@
                    </div>
                </div>

                @* 進階搜尋鍵 *@
                <div class="searchPanel">
                    <button class="btn btn-primary adSearchBtn">搜尋</button>
                </div>
            </div>
        </div>
    </div>

        <div class="header-button">
            <button type="button">註冊</button>
            <button type="button">登入</button>
        </div>
    </div>

    <div id="container">
        <!--邊條選項欄-->
        <div class="sidebox">
            <ul>
                <li>
                    <a href="/Homepage/Main">
                        <div class="menuItem">
                            <i class="fa fa-home"></i>
                            <span>首頁</span>
                        </div>
                    </a>
                </li>
                <li class="side-li-serach-img">
                    <a href="/Search/Kinds">
                        <div class="menuItem">
                            <i class="fa fa-search"></i>
                            <span>音樂分類</span>
                        </div>
                    </a>
                </li>
                <li class="side-li-mymusiclibiary-img">
                    <a>
                        <div class="menuItem">
                            <i class="fa fa-music"></i>
                            <span>你的音樂庫</span>
                        </div>
                    </a>
                </li>
                <li class="side-li-twoPlayer">
                    <div class="menuItem">
                        <a href="/TwoPlayer/DemoPlayer">雙重播放器測試</a>
                    </div>
                </li>
            </ul>
        </div>


        <!--內容主體-->
        <div class="mainbox">
            @RenderBody()
        </div>
    </div>

    <div id="footer">
        <div id="playerPanel">
            <!-- 封面與歌名div -->
            <figure id="playerfigure">
                <img src="#" class="title">
                <figcaption id="playerfigcaption" class="description title">這裡放專輯名</figcaption>
            </figure>
            <div id="titlePanel">
                <span class="description">這裡放歌名</span><br>
                <span id="singer">這裡放歌手</span>
            </div>

            <!-- 基本按鍵div -->
            <div id="btnPanel">
                <div id="random" class="btn"><img src="/PlayerImg/random.png"></div>
                <div id="prev" class="btn"><img src="/PlayerImg/prev.png"></div>
                <div id="play" class="btn"><img src="/PlayerImg/play.png"></div>
                <div id="pause" class="btn"><img src="/PlayerImg/pause.png"></div>
                <div id="next" class="btn"><img src="/PlayerImg/next.png"></div>
