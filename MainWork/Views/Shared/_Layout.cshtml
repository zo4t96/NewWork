            </div>

            <!-- 音量條div -->
            <div id="volumePanel">
                <img src="/PlayerImg/sound.png" data-sound="volumeImg" id="volumeImg" class="sound">
                <img src="/PlayerImg/mute.png" data-sound="muteImg" id="muteImg" class="sound">
                <input id="volumeBar" type="range" min="0" max="1" step="0.01" value="0.5">
            </div>

            <!--播放器本體-->
            <audio id="mainPlayer" src="/Music/‪02 紅魔 「Un-demystified Fantasy」.mp3"></audio>
        </div>
    </div>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    @*<script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>*@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script>

        /////////////////
        ///主播放器相關///
        /////////////////

        var player;//主播放器
        var playing;//是否正在播放中
        var totalTime;//本次歌曲的秒數總長度
        var nowVolume;//當前音量

        //避免頁面刷新時既有變數被重置或重複給予事件
        if (player == null) {
            player = document.getElementById("mainPlayer");
            playing = false;
            $("#pause").css("display", "none");
            $("#muteImg").css("display", "none");
            player.volume = 0.5;
            $("#volumeBar").css({ "background-image": "-webkit-linear-gradient(left , #5599FF 50%,#fff 0px)" });

            nowVolume = player.volume;

            //初始化播放器內音樂的總長度，並將值傳給進度條
            player.addEventListener("durationchange", function () {
                totalTime = parseInt(player.duration);
                $("#progress").prop("max", totalTime);

                let m1 = parseInt(totalTime / 600);
                let m2 = parseInt(totalTime / 60);
                let s1 = parseInt(totalTime % 60 / 10);
                let s2 = parseInt(totalTime % 60 % 10);
                $("#totalTime").text(`${m1}${m2}:${s1}${s2}`);
            });

            //播放器的timer
            player.addEventListener("timeupdate", function () {
                $("#progress").prop("value", player.currentTime);

                let now = player.currentTime;
                let m1 = parseInt(now / 600);
                let m2 = parseInt(now / 60);
                let s1 = parseInt(now % 60 / 10);
                let s2 = parseInt(now % 60 % 10);
                $("#nowTime").text(`${m1}${m2}:${s1}${s2}`);

                //進度條的變化
                let persent = now / totalTime * 100;
                let p = parseInt($("#progress").css("width")) / 100;
                let f = persent * p;
                $("#progress").css({ "background-image": `-webkit-linear-gradient(left , #5599FF ${f}px,#fff 0px)` });
            });

            //音樂播放鍵
            $("#play").click(function () {
                player.play();
                playing = true;
                $("#play").css("display", "none");
                $("#pause").css("display", "inline-block")
            })

            //音樂暫停鍵
            $("#pause").click(function () {
                player.pause();
                playing = false;
                $("#play").css("display", "inline-block");
                $("#pause").css("display", "none")
            })

            //調整音量時能立即更動而不用放開滑鼠
            $("#volumeBar").on('input', function () {
                $(this).trigger("change");
            })
            //音量調節鍵
            $("#volumeBar").on('change', function () {
                player.volume = this.value;
                let v = this.value * 100;
                $("#volumeBar").css({ "background-image": `-webkit-linear-gradient(left , #5599FF ${v}%,#fff 0px)` });
                if (player.volume == 0) {
                    $("#volumeImg").css("display", "none");
                    $("#muteImg").css("display", "inline-block");
                }
                else {
                    $("#volumeImg").css("display", "inline-block");
                    $("#muteImg").css("display", "none");
                }
            });

            //靜音鍵
            $(".sound").click(function () {
                let data = $(this).data("sound");
                switch (data) {
                    case "volumeImg":
                        nowVolume = player.volume;
                        player.volume = 0;
                        $("#volumeBar").prop("value", 0);
                        $("#volumeBar").css({ "background-image": `-webkit-linear-gradient(left , #5599FF ${0}%,#fff 0px)` });
                        $("#volumeImg").css("display", "none");
                        $("#muteImg").css("display", "inline-block");
                        break;

                    case "muteImg":
                        player.volume = nowVolume;
                        $("#volumeBar").prop("value", nowVolume);
                        $("#volumeBar").css({ "background-image": `-webkit-linear-gradient(left , #5599FF ${nowVolume * 100}%,#fff 0px)` });
                        $("#volumeImg").css("display", "inline-block");
                        $("#muteImg").css("display", "none");
                        break;
                }
            });

            //選擇進度條後立刻發動值的change(否則會出現延遲，造成改值失敗)
            $("#progress").on('input', function () {
                player.pause();
                $(this).trigger("change");
            })
            //音樂進度條改變
            $("#progress").change(function () {
                player.currentTime = this.value;
            });
            //拖動進度條直到放開滑鼠才重新開始播放
            $("#progress").mouseup(function () {
                if (playing)
                    player.play();
            });

            /////////////////////
            ///AJAX切換頁面相關///
            /////////////////////


            //為了使用ajax所寫的自定義方法
            //link參數為url字串
            function changePage(link) {

                //$(".mainbox").load(link);
                //更新老師上課說法
                //load可以讀取一個頁面中的部分位置如"AAA/BBB #div1>img"這種寫法
                //意為讀取該網頁id為div1中的img
                //但是mvc本身有PartailView這個功能，因此應該用不到load

                //下面的data是ajax方法發動後從對定應的action裡取得的html語法，必須事先寫好
                //$.get(link, function (data) {
                //    $(".mainbox").html(data);
                //})

                //如果有上傳參數到後端則使用$.post的ajax方法
                //不過為了配合重新整理頁面不跑掉的系統，暫時是一律使用$.post來處理所有ajax
                $.post(link , { ajax: true }, function (data) {
                    $(".mainbox").html(data);
                })
            }

            //點連結將主容器切換到新頁面，這個的內容會根據連結的不同有所改變
            $("a").on("click", function (e) {
                let link = $(this).attr("href");
                changePage(link);
                //給予網頁虛擬網址，隨便打都能過，但是最好讓他跟實際變換的網址同名
                //第一個參數要帶狀態物件，若點選的連結沒有參數指定就可以給null
                //反過來說，有參數指定的連結最好額外再寫一個ajax判斷式比較好
                history.pushState(null, null, link);
                //因為要將網頁連結變成自定義的切換模式，所以記得取消原本a標籤的超連結功能
                e.preventDefault();
            });

            //每當點選瀏覽器的上一頁下一頁便會發動此事件
            //location.pathname為動作完成後所得到的該頁url
            window.addEventListener('popstate', function (e) {
                changePage(location.pathname);
            })

            /////////////////
            ///搜尋條棒相關///
            /////////////////

            //進階搜尋內容的初始化
            $.getJSON('@Url.Action("SetTypes","Search")', function (datas) {
                $.each(datas, function (name, value) {
                    $("#adType").append(`<option value="${value.fTypeID}">${value.fTypeName}</option>`)
                })
            })
            $.getJSON('@Url.Action("SetKinds","Search")', function (datas) {
                $.each(datas, function (name, value) {
                    $(".advancedBlock").append(`<div class="selectedItem" data-kind="${value.KindName}">${value.KindName}</div>`);
                    $(".selectableItemPanel").append(`<div class="selectableItem" data-kind="${value.KindName}">${value.KindName}</div>`)
                })
                $(".advancedBlock").append('<div class="itemAdd"><i class="fa fa-plus"></i></div>');

                $(".itemAdd").click(function () {
                    $(".selectableItemPanel").toggleClass("adSwitch");
                })

                //初始化進階面板曲風選項的顏色
                var color = ["#6bc477", "#C158E8", "#6E7AFF", "#58D5E8", "#A6FFB4", "#C7E874", "#FFE98B"
                  , "#cc9250", "#F587DE", "#FFAB9A", "#FFF56F", "#B5FF5A", "#6796B8", "#7A67B8", "#FFBE50"
                  , "#61FFAD", "#C6565C", "#58C782", "#B9F0FD", "#B6CC6E", "#6EBBCC"];
                  var block = $(".selectableItemPanel").children(".selectableItem");
                  var block2 = $(".advancedBlock").children(".selectedItem");
                for (let i = 0; i < block.length; i++) {
                    block.eq(i).css("background-color", color[i]);
                    block.eq(i).css("background-image", `linear-gradient(to bottom,#16161660 100%,${color[i]})`);
                    block2.eq(i).css("background-color", color[i]);
                    block2.eq(i).css("background-image", `linear-gradient(to bottom,#16161660 100%,${color[i]})`);
                }
                //選擇曲風
                $(".selectableItem").on("click", function () {
                    let kindName = $(this).data("kind");
                    $(this).css("display", "none");
                    $(".advancedBlock").find(`[data-kind = '${kindName}']`).css("display", "inline-block");
                    $(".selectableItemPanel").toggleClass("adSwitch");
                })
                //點擊選擇的曲風即可取消
                $(".selectedItem").on("click", function () {
                    let kindName = $(this).data("kind");
                    $(this).css("display", "none");
                    $(".selectableItemPanel").find(`[data-kind = '${kindName}']`).css("display", "inline-block");
                })
            }) 

            //開啟進階搜尋版面、開啟曲風選擇版面
            $("#advancedBtn").click(function () {
                $(".advancedPanel").toggleClass("adSwitch");
            })
            $(".itemAdd").click(function () {
                $(".selectableItemPanel").toggleClass("adSwitch");
            })
            //點選搜尋區域以外的地方會自動關閉進階搜尋版面(很礙眼)
            $(document).mousedown(function (e) {
                if ($(e.target).parents(".searchBar").length == 0 &&
                    $(e.target).parents(".ui-menu-item").length == 0) {
                    $(".advancedPanel").removeClass("adSwitch");
                }
                if ($(e.target).parents(".selectableItemPanel").length == 0) {
                    $(".selectableItemPanel").removeClass("adSwitch");
                }
            })

            //關鍵字搜尋(只查找歌名並輸出專輯與歌名(專輯可能重複))
            //用trim()除去空白字串
            $(".searchBtn").click(function () {
                let keyword = $(".searchtxt").val().trim();
                if (keyword == "") {
                    return;
                }
                let url = `@Url.Action("Result", "Search")`;
                let stateObj = { keyword: keyword, ajax: true };
                $.post(url, stateObj, function (data) {
                    $(".mainbox").html(data);
                });
                history.pushState(stateObj, null, url);
            })

            //進階搜尋(找出符合條件的專輯(專輯不重複))
            $(".adSearchBtn").click(function () {
                $(".advancedPanel").toggleClass("adSwitch");
                let adSong = $("#adSong").val().trim();
                let adSinger = $("#adSinger").val().trim();
                let adGroup = $("#adGroup").val().trim();
                let adComposer = $("#adComposer").val().trim();
                let adAlbum = $("#adAlbum").val().trim();
                let adType = $("#adType").val();
                let adKinds = "";
                $(".advancedBlock").children(".selectedItem").each(function () {
                    if ($(this).css("display") == "inline-block") {
                        adKinds += ($(this).data("kind") + ",");
                    }
                })
                let url = `@Url.Action("AdvancedResult", "Search")`
                let stateObj = {
                    adSong: adSong,
                    adSinger: adSinger,
                    adGroup: adGroup,
                    adComposer: adComposer,
                    adAlbum: adAlbum,
                    adType: adType,
                    adKinds: adKinds,
                    ajax: true
                }
                $.post(url, stateObj, function (data) {
                    $(".mainbox").html(data);
                });
                history.pushState(stateObj,null,url);
            })

            //所有搜尋text實作autocomplete
            $(".searchtxt,#adSong").autocomplete(
                {
                    source: '@Url.Action("AutoCompleteSong", "Search")'
                });
            $("#adSinger").autocomplete(
                {
                    source: '@Url.Action("AutoCompleteSinger", "Search")'
                });
            $("#adGroup").autocomplete(
                {
                    source: '@Url.Action("AutoCompleteGroup", "Search")'
                });
            $("#adComposer").autocomplete(
                {
                    source: '@Url.Action("AutoCompleteComposer", "Search")'
                });
            $("#adAlbum").autocomplete(
                {
                    source: '@Url.Action("AutoCompleteAlbum", "Search")'
                });      
        }
    </script>

</body>
</html>